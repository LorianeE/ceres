    "@tsed/di": "6.14.1",
    "@tsed/exceptions": "6.14.1",
    "@tsed/json-mapper": "6.14.1",
    "@tsed/mongoose": "6.14.1",
    "@tsed/passport": "6.14.1",
    "@tsed/platform-express": "6.14.1",
    "@tsed/schema": "6.14.1",
    "@tsed/swagger": "6.14.1",
    "@types/express-session": "^1.17.0",
    "@types/mongoose": "5.10.0",
    "@types/passport-facebook": "^2.1.9",
    "@types/serve-favicon": "2.5.0",
    "@types/sinon": "^9.0.0",
    "@types/swagger-schema-official": "2.0.20",
    "ajv": "6.12.6",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.1.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "method-override": "3.0.0",
    "mongoose": "5.9.27",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
