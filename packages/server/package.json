    "@tsed/swagger": "6.9.0",
    "@tsed/exceptions": "6.9.0",
    "@tsed/platform-express": "6.9.0",
    "@types/express-session": "^1.17.0",
    "@types/mongoose": "5.7.11",
    "@types/passport-facebook": "^2.1.9",
    "@types/serve-favicon": "^2.5.0",
