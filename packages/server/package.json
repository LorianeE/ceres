{
  "name": "@ceres/server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn tsc",
    "docker:build": "yarn build && docker-compose build",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "@tsed/ajv": "5.46.0",
    "@tsed/common": "5.46.0",
    "@tsed/core": "5.46.0",
    "@tsed/di": "5.46.0",
    "@tsed/mongoose": "5.46.0",
    "@tsed/swagger": "5.46.0",
    "@types/mongoose": "5.7.11",
    "@types/sinon": "^9.0.0",
    "@types/swagger-schema-official": "2.0.20",
    "ajv": "6.12.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.1.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "method-override": "3.0.0",
    "mongoose": "5.9.9",
    "sinon": "^9.0.2",
    "ts-httpexceptions": "^4.1.0"
  },
  "devDependencies": {
    "@tsed/cli-plugin-jest": "1.8.1",
    "@tsed/cli-plugin-tslint": "1.8.1",
    "@tsed/testing": "5.46.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/method-override": "0.0.31",
    "@types/node": "13.11.1",
    "@types/supertest": "2.0.8",
    "concurrently": "5.1.0",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "lint-staged": "10.1.3",
    "nodemon": "2.0.3",
    "prettier": "2.0.4",
    "supertest": "4.0.2",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
